{
  "name": "pytracex",
  "displayName": "PyTracerX",
  "description": "Python program comprehension tool for novice programmers",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.83.0"
  },
  "categories": ["Other"],
  "activationEvents": ["onCommand:pytracex.analyzeCurrentFile"],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "pytracex.analyzePythonFile",
        "title": "Analyze Python File"
      }
    ],
      "keybindings": [
          {
              "command": "pytracex.analyzePythonFile",
              "key": "ctrl+alt+x",
              "mac": "shift+cmd+f"
          }
      ]
  },
  "scripts": {
      "postinstall": "node ./node_modules/vscode/bin/install",
      "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/vscode": "^1.83.0",
    "@types/mocha": "^10.0.2",
    "@types/node": "18.x",
    "eslint": "^8.50.0",
    "glob": "^10.3.3",
    "mocha": "^10.2.0",
    "typescript": "^5.2.2",
    "@vscode/test-electron": "^2.3.4"
  }
}